<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extensions">
  <schema path="/org/gnome/shell/extensions/launchandexpose/" id="org.gnome.shell.extensions.launchandexpose">
    <key name="show-launchpad-icon" type="b">
      <default>true</default>
      <summary>Show LaunchPad Icon</summary>
      <description>
        Show Launcpad icons on top left
      </description>
    </key>
    <key name="label-launchpad-text" type="s">
      <default>"LaunchPad"</default>
      <summary>LaunchPad label</summary>
      <description>
        Set Launcpad Text Label on top left
      </description>
    </key>
    <key name="show-expose-icon" type="b">
      <default>true</default>
      <summary>Show Expose</summary>
      <description>
        Show Expose icons on top right 
      </description>
    </key>
    <key name="icon-expose-path" type="s">
      <default>'/usr/share/icons/HighContrast/24x24/emblems/emblem-photos.png'</default>
      <summary>Expose Icon Path.</summary>
      <description>custom Expose icon path.</description>
    </key>
    <key name="show-launchpad-text" type="b">
      <default>true</default>
      <summary>Show LaunchPad Label</summary>
      <description>
        Show LaunchPad Label Text On Top Left
      </description>
    </key>
    <key name="disable-hotcorner" type="b" >
      <default>false</default>
      <summary>Disable Hot Corner.</summary>
      <description>Disable Activities Hot Corner.</description>
    </key>
      <key name="icon-launchpad-path" type="s">
      <default>'/usr/share/icons/HighContrast/24x24/places/start-here.png'</default>
      <summary>LauncPad Icon Path.</summary>
      <description>custom LauncPad icon path.</description>
    </key>
  </schema>
</schemalist>
